<form action="/" method="post" enctype="multipart/form-data">
    <h3>Upload an image.</h3>
    <input type="file" name="imageFile" accept="image/*">
    <input type="submit">
</form>

<div>
    {{#each result}}
        <p>↓に保存された画像が出力されます</p>
        {{!-- <img src="sea.jpg" alt="" /> --}}
        <p>{{this.imageName}}</p>
    {{/each}}
</div>